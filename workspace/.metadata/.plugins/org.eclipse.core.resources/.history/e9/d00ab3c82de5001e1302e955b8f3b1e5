package com.yourcompany.bmo.model;
import java.time.*;

import javax.persistence.*;

import lombok.*;
@Embeddable
@Getter @Setter // This makes all fields below publicly accessible
public class Sports {
	
	
	@Required@Column(length=32)
    String nomEquipe1;
	@Column(length=32)
    String nomEquipe2;
	
	LocalDate date;

}
