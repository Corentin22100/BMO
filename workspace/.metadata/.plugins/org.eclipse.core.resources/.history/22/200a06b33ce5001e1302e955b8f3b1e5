package com.yourcompany.bmo.model;

import javax.persistence.*;

import org.openxava.annotations.*;

import lombok.*;

@Entity @Getter @Setter
public class Resultats{
	
	@ManyToOne( // The reference is persisted as a database relationship
			fetch=FetchType.LAZY) // The reference is loaded on demand
	@DescriptionsList
	@Required
    Evenements evenementSportif; // A regular Java reference
	
	@Embedded
	PariSimple résultatPariSimple;

	@Embedded
	PariAvanceBasket résultatPariAvancésBasket;
	
	@Embedded
	PariAvanceTennis résultatPariAvancésCoursesDeChevaux;
	
	@Embedded
	PariAvanceFoot résultatPariAvancésFoot;
	
	@Embedded
	PariAvanceTennis résultatPariAvancésTennis;
}
